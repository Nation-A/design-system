{
  "name": "@nation-a/icons",
  "version": "0.5.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf src/components",
    "gen:icons": "tsx src/scripts/generate-icons.ts",
    "gen:index": "tsx src/scripts/create-index.ts",
    "prebuild": "pnpm clean && pnpm gen:icons && pnpm gen:index",
    "build": "tsc && vite build --mode production"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.40.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./v3": {
      "types": "./dist/types/v3/index.d.ts",
      "require": "./dist/v3/index.cjs",
      "import": "./dist/v3/index.mjs"
    }
  }
}
